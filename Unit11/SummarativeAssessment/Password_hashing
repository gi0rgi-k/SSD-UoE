import hashlib

"""
Password hashing notebook. Can be useful when repeatedly testing 'Create User' and 'Login' functions.
"""

# Hashing the password "AdminPass" using SHA256
password = "AdminPass"
hashed_password = hashlib.sha256(password.encode()).hexdigest()
print(hashed_password)


# Hashing the password "ClerkTest" using SHA256

password = "ClerkTest"
hashed_password = hashlib.sha256(password.encode()).hexdigest()
print(hashed_password)


# Hashing the password "CustomerTest" using SHA256

password = "CustomerTest"
hashed_password = hashlib.sha256(password.encode()).hexdigest()
print(hashed_password)